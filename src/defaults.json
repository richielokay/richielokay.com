{
    "name": "prod",
    "dest": "dist/prod",
    "src": "app",
    "gzip": false,
    "env": {
        "NODE_ENV": "production",
        "ASSETS_URL": "/assets/"
    },
    "scripts": {
        "debug": false,
        "uglify": false
    },
    "styles": {
        "sourcemaps": false,
        "outputStyle": "nested",
        "includePaths": []
    },
    "html": {
        "lrSnippet": false
    },
    "assets": {
        "src": "assets",
        "dest": "dist/prod/assets"
    }
}
